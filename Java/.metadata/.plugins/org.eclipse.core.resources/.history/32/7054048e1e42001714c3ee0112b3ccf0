
public class Main {
	public static int game(char a,char b){
		if(a==b){
			return 0;
		}
		else if((a=='C'&&b=='J')||(a=='J'&&b=='B')||(a=='B'&&b=='C')){
			return 1;
		}
		else{
			return -1;
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
	}

}
