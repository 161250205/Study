package fxx.solar;

import java.awt.Image;

import fxx.util.Constant;
import fxx.util.GameUtil;

public class Star {
	protected Image I;
	protected double x;
	protected double y;
	protected double width;
	protected double hight;
	public Star(Image i, double x, double y, double width, double hight) {
		I = i;
		this.x = x;
		this.y = y;
		this.width = I.getWidth(null);
		this.hight = I.getHeight(null);
	}
	public Star(String imgPath, double x, double y, double width, double hight) {
		I = GameUtil.getImage(imgPath);
		this.x = x;
		this.y = y;
		this.width = I.getWidth(null);
		this.hight = I.getHeight(null);
	}
}
