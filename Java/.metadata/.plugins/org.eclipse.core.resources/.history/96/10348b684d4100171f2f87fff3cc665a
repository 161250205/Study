package smg;

import java.util.ArrayList;
import java.util.Collection;
import java.util.Scanner;

public class Main {
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	Scanner in = new Scanner(System.in);
	int N = in.nextInt();
	int L = in.nextInt();
	int H = in.nextInt();
	ArrayList <Student> stu1=new ArrayList <Student>();
	ArrayList <Student> stu2=new ArrayList <Student>();
	ArrayList <Student> stu3=new ArrayList <Student>();
	ArrayList <Student> stu4=new ArrayList <Student>();
	for(int i=0;i<N;i++)
	{
		int a,b,c;
		a=in.nextInt();
		b=in.nextInt();
		c=in.nextInt();
		if(b>=H&&c>=H)
		{
			stu1.add(new Student(a,b,c));			
		}
		else if(b>=H&&c>=L)
		{
			stu2.add(new Student(a,b,c));
		}
		else if(b>=L&&c>=L&&b>=c)
		{
			stu3.add(new Student(a,b,c));
		}
		else if(b>=L&&c>=L)
		{
			stu4[count4].getNum(a);
			stu4[count4].getScor1(b);
			stu4[count4].getScor2(c);
			count4++;
		}
	}
	Collection.sort
	
	}

}

class Student{
	private int num;
	private int scor1;
	private int scor2;
	public Student(int num, int scor1, int scor2) {
		this.num = num;
		this.scor1 = scor1;
		this.scor2 = scor2;
	}
	
	
}
