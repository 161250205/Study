package game;

import java.awt.Image;

import fxx.util.Constant;

public class Bullet {
	protected Image img;
	protected double x,y;
	protected double degree;
	protected double speed;
	int width,height;
	
	public Bullet(){
		this.x=Constant.GAME_WIDTH/2;
		this.y=Constant.GAME_HEIGHT/2;
		width=10,height=10;
	}
}
