package smg;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	Scanner in = new Scanner(System.in);
	int N = in.nextInt();
	int L = in.nextInt();
	int H = in.nextInt();
	int [][] student = new int [N][3];
	int count=0;
	for(int i=0;i<N;i++)
	{
		int a,b,c;
		a=in.nextInt();
		b=in.nextInt();
		c=in.nextInt();
		if(b>=L&&c>=L)
		{
			student[L][0]=a;
			student[L][1]=b;
			student[L][2]=c;
			count++;
		}
	}
	}

}
