package smg;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	Scanner in = new Scanner(System.in);
	int N = in.nextInt();
	int L = in.nextInt();
	int H = in.nextInt();
	int [][] student_first = new int [N][3];
	int count_first=0;
	int [][] student_second = new int [N][3];
	int count_second=0;
	int [][] student_third = new int [N][3];
	int count_third=0;
	int [][] student_forth = new int [N][3];
	int count_forth=0;
	for(int i=0;i<N;i++)
	{
		int a,b,c;
		a=in.nextInt();
		b=in.nextInt();
		c=in.nextInt();
		if(b>=H&&c>=H)
		{
			student_first[count_first][0]=a;
			student_first[count_first][1]=b;
			student_first[count_first][2]=c;
			count_first++;
		}
		else if(b>=H&&c>=L)
		{
			student_second[count_second][0]=a;
			student_second[count_second][1]=b;
			student_second[count_second[2]=c;
			count_second++;
		}
	}
	}

}
