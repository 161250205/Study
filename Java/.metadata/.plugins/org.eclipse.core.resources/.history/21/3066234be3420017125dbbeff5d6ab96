package numhole;

public class Main {
	public static int reverse(int num){
		String s=Integer.toString(num);
		int count=1;
		int k=0;
		for(int i=0;i<s.length();i++)
		{
			k=k + ((int)s.charAt(i)-'0')*count;
		}

		return k;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
